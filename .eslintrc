{
    "extends": "standard",
    "parser": "babel-eslint",
    "env": {
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-expressions": 0,
        "no-prototype-builtins": 0,
        "no-async-promise-executor": 0,
        "promise/param-names": 0,
        "no-console": 0,
        "no-shadow": "error",
        "prefer-const": "error",
        "no-param-reassign": "error",
        "space-before-function-paren": 0,
        "array-bracket-spacing": "error",
        "object-property-newline": "error",
        "object-curly-spacing": 1,
        "no-unused-vars": [
            "error",
            {
                "args": "all",
                "argsIgnorePattern": "^_"
            }
        ],
        "object-curly-newline": [
            "error",
            {
                "ObjectExpression": {
                    "minProperties": 1
                },
                "ObjectPattern": {
                    "multiline": true
                },
                "ImportDeclaration": "never",
                "ExportDeclaration": {
                    "multiline": true,
                    "minProperties": 3
                }
            }
        ],
        "no-multiple-empty-lines": [
            "error",
            {
                "max": 1
            }
        ],
        "indent": [
            "error",
            4,
            {
                "ObjectExpression": 1,
                "SwitchCase": 1
            }
        ],
        "comma-style": [
            "error",
            "first"
        ],
        "semi": [
            "error",
            "never"
        ]
    }
}